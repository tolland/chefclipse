    <!-- purge the chefclipse groupId -->
    <profile>
      <id>purge</id>
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>
      <build>
        <plugins>
          <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-dependency-plugin</artifactId>
            <version>2.8</version>
            <executions>
              <execution>
                <id>purge-local-dependencies</id>
                <phase>process-sources</phase>
                <goals>
                  <goal>purge-local-repository</goal>
                </goals>
                <configuration>
                  <DresolutionFuzziness>groupId</DresolutionFuzziness>
                  <includes>
                    <include>org.limepepper.chefclipse</include>
                  </includes>
                  <verbose>true</verbose>
                  <DactTransitively>false</DactTransitively>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>


