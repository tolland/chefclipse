    <!-- izpack creates a install.jar, depends on the build profile -->
    <profile>
      <id>ghpagessite</id>
      <activation>
        <activeByDefault>false</activeByDefault>
      </activation>

      <build>
        <plugins>
          <plugin>
            <groupId>com.github.github</groupId>
            <artifactId>site-maven-plugin</artifactId>
            <version>0.8</version>
            <configuration>
              <message>Creating site for ${project.version}</message>

              <repositoryName>chefclipse</repositoryName>
              <repositoryOwner>limepepper</repositoryOwner>

            </configuration>
            <executions>
              <execution>
                <goals>
                  <goal>site</goal>
                </goals>
                <phase>site</phase>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>


    </profile>



